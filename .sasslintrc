{
  "files": {
    "include": "**/*.scss"
  },
  "rules": {
    "border-zero": 0,
    "brace-style": 0,
    "class-name-format": [
      1,
      {
        "convention": "hyphenatedlowercase"
      }
    ],
    "clean-import-paths": [
      1,
      {
        "filename-extension": false,
        "leading-underscore": true
      }
    ],
    "empty-line-between-blocks": [
      1,
      {
        "ignore-single-line-rulesets": false
      }
    ],
    "force-attribute-nesting": 1,
    "force-element-nesting": 1,
    "force-pseudo-nesting": 0,
    "final-newline": 0,
    "function-name-format": [
      1,
      {
        "convention": "hyphenatedlowercase"
      }
    ],
    "hex-length": [
      1,
      {
        "style": "short"
      }
    ],
    "indentation": [
      1,
      {
        "size": "2"
      }
    ],
    "no-css-comments": 1,
    "no-trailing-zero": 1,
    "no-important": 0,
    "no-vendor-prefixes": 0,
    "no-color-literals": 0,
    "mixin-name-format": 0,
    "no-qualifying-elements": 0,
    "nesting-depth": [
      1,
      {
        "max-depth": 3
      }
    ],
    "space-after-colon": [
      1,
      {
        "include": true
      }
    ],
    "space-after-comma": [
      1,
      {
        "include": true
      }
    ],
  "space-before-bang": [
      1,
      {
        "include": true
      }
    ],
    "space-before-brace": [
      1,
      {
        "include": true
      }
    ],
    "quotes": 0,
    "space-before-colon": 1,
    "trailing-semicolon": 1,
    "url-quotes": 1,
    "zero-unit": 1,
    "property-sort-order": 0
  }
}